#first as the line graph of the subdivision
g1=g.copy()
g.subdivide_edges(g.edges(labels=False),1);
g=g.line_graph();
print(g.wiener_index())
for x in g.automorphism_group(return_group=False, orbits=True, algorithm='sage'):
    h=g.copy()
    h.delete_vertex(x[0])
