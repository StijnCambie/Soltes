#alternatively, using its definition using arcs
def ArcG(g):
    S=set();
    E2=set();
    n=g.order();
    E=g.edges(labels=False);
    for (i,j) in E:
        S.add((i,j));
        S.add((j,i));
        E2.add(((i,j),(j,i)));
    for (a,b) in Combinations(S,2):
        if a[0]==b[0]:
            E2.add((a,b))
    
    h = Graph()
    h.add_vertices(S)
    h.add_edges(E2)   
    return(h) 

g=ArcG(g1);
print(g.wiener_index())
for x in g.automorphism_group(return_group=False, orbits=True, algorithm='sage'):
    h=g.copy()
    h.delete_vertex(x[0])
    print(h.wiener_index())
